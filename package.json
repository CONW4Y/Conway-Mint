{
  "name": "conway-deploy",
  "version": "0.1.0",
  "description": "A self-sustaining AI agent that autonomously deploys memecoins and utility tokens on Solana, earns trading fees, and uses those fees to fund its own existence on Conway Cloud.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/skills/token-launcher/index.ts",
    "clean": "rm -rf dist",
    "setup:solana": "bash scripts/setup-solana.sh",
    "fund:devnet": "bash scripts/fund-devnet.sh",
    "lint": "eslint src/ --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "memecoin",
    "token-deployer",
    "ai-agent",
    "automaton",
    "conway",
    "defi",
    "pump-fun",
    "raydium",
    "jupiter"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.3.0",
    "@raydium-io/raydium-sdk-v2": "^0.1.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "bs58": "^6.0.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "eslint": "^9.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
